        <name>autopopulating location and service grp</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || !newValue) {
        return;
    }

    g_form.getReference('machine', function(machineObj) {
        if (machineObj) {
            g_form.setValue('location', machineObj.location);
            g_form.setValue('service_group', machineObj.assignment_group);
        }
    });
}
]]></script>
